### EPSET PRELS: (PRimitive ELabs)

# CONDITIONS

sp {elaborate*props*epset*delta*cond*eq
    (state <s> ^props-epset.delta <d>)
    (<d> ^condition <c>
         ^cond-id1 <cid1>
         ^cond-id2 <cid2>)
    (<cid1> ^name <cname>
            ^id1 <id1>)
    (<cid2> ^name <cname>
            ^id2 <id2>)
    (<c> ^prop-type equality
         ^name <cname>
         ^attr1 <attr1>
         ^attr2 <attr2>)
    (<id1> ^<attr1> <val>)
    (<id2> ^<attr2> <val>)
-->
    (<d> ^cond-result <cres>)
    (<cres> ^name <cname>
            ^result success)
}
